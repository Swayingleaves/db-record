<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dbrecord.mapper.SqlRecordMapper">

    <resultMap id="BaseResultMap" type="com.dbrecord.entity.domain.SqlRecord">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="projectId" column="project_id" jdbcType="BIGINT"/>
        <result property="sqlContent" column="sql_content" jdbcType="LONGVARCHAR"/>
        <result property="executionResult" column="execution_result" jdbcType="LONGVARCHAR"/>
        <result property="executionTime" column="execution_time" jdbcType="INTEGER"/>
        <result property="status" column="status" jdbcType="TINYINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,project_id,sql_content,execution_result,execution_time,status,user_id,create_time
    </sql>

</mapper> 